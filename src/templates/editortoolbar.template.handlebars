<div id="o-editor-toolbar" class="o-control o-toolbar o-padding-horizontal-8 o-rounded-top o-hidden">
    <select name="layer-dropdown">
      {{#each this}}
        <option value="{{this.value}}">{{this.name}}</option>
      {{/each}}
    </select>
    <button id="o-editor-draw" class="o-btn-3" type="button" name="button">
        <svg class="o-icon-fa-pencil">
            <use xlink:href="#fa-pencil"></use>
        </svg>
    </button>
    <button id="o-editor-attribute" class="o-btn-3" type="button" name="button">
      <svg class="o-icon-fa-font">
          <use xlink:href="#fa-font"></use>
      </svg>
    </button>
    <button id="o-editor-delete" class="o-btn-3" type="button" name="button">
      <svg class="o-icon-fa-trash">
          <use xlink:href="#fa-trash"></use>
      </svg>
    </button>
    <button id="o-editor-close" class="o-btn-3" type="button" name="button">
        <svg class="o-icon-fa-times">
            <use xlink:href="#fa-times"></use>
        </svg>
    </button>
</div>
