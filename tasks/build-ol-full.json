{
  "exports": ["*"],
  "umd": true,
  "compile": {
    "externs": [
      "externs/closure-compiler.js",
      "externs/geojson.js",
      "externs/esrijson.js",
      "externs/oli.js",
      "externs/olx.js",
      "externs/proj4js.js",
      "externs/tilejson.js",
      "externs/topojson.js"
    ],
    "define": [
      "ol.ENABLE_DOM=false",
      "ol.ENABLE_WEBGL=true",
      "ol.ENABLE_PROJ4JS=true",
      "ol.ENABLE_IMAGE=false",
      "goog.DEBUG=false"
    ],
    "extra_annotation_name": [
      "api", "observable"
    ],
    "compilation_level": "SIMPLE_OPTIMIZATIONS",
    "manage_closure_dependencies": true
  }
}